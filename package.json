{
  "name": "dragoncord-electron",
  "version": "1.0.0",
  "description": "Dragoncord | Something better than default discord",
  "main": "main.js",
  "scripts": {
    "start": "electron . --unhandled-rejections=strict --trace-warnings %1",
    "plug": "cd injectors & node index.js plug",
    "unplug": "cd injectors & node index.js unplug",
    "update": "git pull",
    "settings-server": "python ./dragoncord/pages/settings/settings-server.py"
  },
  "repository": "https://github.com/Dragoncord-for-discord/dragoncord.git",
  "author": "dragonfire-community",
  "license": "ISC",
  "devDependencies": {
    "electron": "11.0.0",
    "electron-packager": "^13.1.1"
  },
  "dependencies": {
    "@electron/remote": "^2.0.8",
    "@sentry/electron": "^3.0.7",
    "custom-electron-titlebar": "^4.1.0",
    "electron-webrtc": "^0.3.0",
    "got": ">=11.8.5",
    "open": "^8.4.0",
    "source-map-support": "^0.5.21",
    "transparent-titlebar": "^0.2.0"
  }
}
